      <row bottom="minor">
        <cell>
          <m>2</m>
        </cell>
        <cell></cell>
      </row>
    </tabular>
  </table>
  <p>We will test five values for
    <m>x</m>. Any five can be used
  </p>
</sidebyside>
<sidebyside widths="30% 70%" valign="middle">
  <table>
    <tabular left="minor" top="minor">
      <col halign="center" right="minor"/>
      <col halign="center" right="minor"/>
      <!--  -->
      <row bottom="minor">
        <cell>
          <m>x</m>
        </cell>
        <cell>
          <m>y</m>
        </cell>
      </row>
      <row bottom="minor">
        <cell>
          <m>-2</m>
        </cell>
        <cell>2</cell>
      </row>
      <row bottom="minor">
        <cell>
          <m>-1</m>
        </cell>
        <cell>1</cell>
      </row>
      <row bottom="minor">
        <cell>
          <m>0</m>
        </cell>
        <cell>0</cell>
      </row>
      <row bottom="minor">
        <cell>
          <m>1</m>
        </cell>
        <cell>1</cell>
      </row>
      <row bottom="minor">
