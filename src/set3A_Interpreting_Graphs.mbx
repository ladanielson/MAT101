</table>
<sidebyside widths="40% 55%" valign="middle">
  <table>
    <tabular left="minor" top="minor">
      <col halign="center" right="minor"/>
      <col halign="center" right="minor"/>
      <!--  -->
      <row bottom="minor">
        <cell>
          <m>x</m>
        </cell>
        <cell>
          <m>y</m>
        </cell>
      </row>
      <row bottom="minor">
        <cell>
          <m>-3</m>
        </cell>
        <cell>
          <m>-4</m>
        </cell>
      </row>
      <row bottom="minor">
        <cell>
